    <script>
        document.addEventListener('DOMContentLoaded', () => {
            // Apply Coupon Event Listener
            const applyCouponBtn = document.getElementById('applyCouponBtn');
            if (applyCouponBtn) {
                applyCouponBtn.addEventListener('click', async () => {
                    const couponCode = document.getElementById('couponCode').value;
                    try {
                        const response = await fetch('/apply_coupon', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({ couponCode })
                        });

                        const result = await response.json();
                        if (response.status === 200) {
                            Swal.fire({
                                icon: 'success',
                                title: 'Coupon Applied!',
                                text: result.message
                            }).then(() => {
                                window.location.reload(); // Reload to reflect the applied coupon
                            });
                        } else {
                            Swal.fire({
                                icon: 'error',
                                title: 'Error',
                                text: result.message
                            });
                        }
                    } catch (error) {
                        console.error('Error applying coupon:', error);
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: 'An unexpected error occurred.'
                        });
                    }
                });
            }

            // Remove Coupon Event Listener
            const removeCouponBtn = document.getElementById('removeCouponBtn');
            if (removeCouponBtn) {
                removeCouponBtn.addEventListener('click', async () => {
                    try {
                        const response = await fetch('/remove_coupon', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            }
                        });

                        const result = await response.json();
                        if (response.status === 200) {
                            Swal.fire({
                                icon: 'success',
                                title: 'Coupon Removed!',
                                text: result.message
                            }).then(() => {
                                window.location.reload(); // Reload the page to update the UI
                            });
                        } else {
                            Swal.fire({
                                icon: 'error',
                                title: 'Error',
                                text: result.message
                            });
                        }
                    } catch (error) {
                        console.error('Error removing coupon:', error);
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: 'An unexpected error occurred.'
                        });
                    }
                });
            }
        });





    async function quantityUpdate(productId, status) {
        console.log(productId, status);
        try {
            let response = await fetch('/cart/quantity', {
                method: "PUT",
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ productId: productId, status: status })
            });

            if (!response.ok) {
                throw new Error('Network response was not ok');
            }

            let data = await response.json();
            console.log('Data:', data);

            if (data.message === 'product exceeded') {
                await Swal.fire('Product stock exceeded', 'Cannot increase quantity', 'error');
                location.reload();
            } else if (data.message === 'Max 10') {
                await Swal.fire('Maximum quantity', 'Cannot increase quantity more than 10', 'error');
                location.reload();
            } else if (data.message === 'Min 1') {
                await Swal.fire('Minimum quantity', 'Cannot decrease quantity below 1', 'error');
                location.reload();
            } else {
                document.getElementById(`quantity${data.productData._id}`).innerText = data.quantity;
                document.getElementById(`totalPrice${data.productData._id}`).innerText = '₹ ' + (data.quantity * data.finalPrice);
                document.getElementById(`price${data.productData._id}`).innerText = '₹ ' + data.finalPrice;
                document.getElementById('subTotal').innerText = '₹ ' + data.total;
                document.getElementById('total').innerText = '₹ ' + (data.total+300-data.discount);
                if(data.discount){
                    document.getElementById('discount').innerText = '-₹ ' + (data.discount);
                }
                
                console.log(data.total);
                console.log('Discount:',data.discount);
                console.log('Quantity updated successfully');
            }

        } catch (error) {
            console.error('Error:', error);
            await Swal.fire('Error', 'Something went wrong. Please try again later.', 'error');
        }
    }



    async function removeProduct( productId,productPrice) {
        console.log( productId, productPrice);
        const result = await Swal.fire({
            title: 'Are you sure?',
            text: "Do you want to remove this product from the cart?",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, remove it!'
            });

            if (result.isConfirmed) {
                const response = await fetch('/cart/remove_product', {
                    method: 'DELETE',
                    headers: {
                    'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({ productId: productId, productPrice: productPrice })
                });

            if(response.ok) {
                await Swal.fire(
                'Removed!',
                'The product has been removed.',
                'success'
                );
                window.location.reload();
            }else {
                await Swal.fire(
                    'Error!',
                    'There was a problem removing the product.',
                    'error'
                );
            }
        }
    }
