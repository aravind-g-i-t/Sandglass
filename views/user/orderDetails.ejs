    <script>
        function showPaymentOptions() {
            // Hide the "Pay now" button
            document.getElementById('payNowButton').style.display = 'none';
            
            // Show the payment options
            document.getElementById('paymentOptions').style.display = 'block';
        }
        
        function cancelPayment() {
            // Show the "Pay now" button
            document.getElementById('payNowButton').style.display = 'block';
        
            // Hide the payment options
            document.getElementById('paymentOptions').style.display = 'none';
        }

        async function payNow(id, totalPrice){
            console.log('payNow working');
            
            const selectedPaymentMethod = document.querySelector('input[name="payment-method"]:checked');
            if (!selectedPaymentMethod) {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Please select a payment method.'
                });
                return;
            }
            const paymentMethod = selectedPaymentMethod.value;
            console.log('Payment method :',paymentMethod);
            if (paymentMethod === "Wallet") {
                console.log('Fetch paybywallet');
                
                fetch('/pay-by-wallet', {
                    method: 'PATCH',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({
                        id,
                        totalPrice
                    })
                })
                .then(response => response.json())
                .then(data => {
                    if (data.message === "Success") {
                        console.log('paybywallet success');
                        
                        Swal.fire({
                            title: 'Payment Successful',
                            text: 'Your payment has been done successfully!',
                            icon: 'success',
                            confirmButtonText: 'OK'
                        }).then(result => {
                            if (result.isConfirmed) {
                                window.location.href = `/order_details?orderId=${id}`; // Replace '/' with your home page URL
                            }
                        });
                    } else {
                        Swal.fire({
                            title: 'Payment failed',
                            text: data.message,
                            icon: 'error',
                            confirmButtonText: 'OK'
                        }).then(result => {
                            if (result.isConfirmed) {
                                window.location.href = `/order_details?orderId=${id}`; // Redirect to cart page
                            }
                        });
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                    Swal.fire({
                        title: 'Order not placed',
                        text: 'An unexpected error occurred. Please try again later.',
                        icon: 'error',
                        confirmButtonText: 'OK'
                    }).then(() => {
                        window.location.href = `/order_details?orderId=${id}`; // Redirect to cart page
                    });
                });
            } else {
                try {
                    const orderId = await createRazorpayOrder(totalPrice);
                    if (orderId) {
                        console.log('orderId is there')
                        handleRazorpayNow(orderId, totalPrice,id);
                    }
                } catch (error) {
                    console.error('Error:', error);
                    Swal.fire({
                        title: 'Order not placed',
                        text: 'An unexpected error occurred. Please try again later.',
                        icon: 'error',
                        confirmButtonText: 'OK'
                    }).then(() => {
                        window.location.href = `/order_details?orderId=${id}`; // Redirect to cart page
                    });
                }
            }
        }

        async function createRazorpayOrder(totalPrice) {
            const settings = {
                url: "/payment/razorpay",
                method: "POST",
                headers: {
                    "Content-Type": "application/json"
                },
                data: JSON.stringify({
                    amount: totalPrice * 100 // Convert to paisa
                }),
            };

            const response = await $.ajax(settings);
            return response.orderId;
        }

        function handleRazorpayNow(orderId, totalPrice, id) {
            console.log('handleRazorpayNOw:',orderId,totalPrice,id);
            
            const options = {
                key: "rzp_test_7DylGE19pXY4Lf", // Replace with your Razorpay Key ID
                amount: totalPrice * 100, // Amount in paisa
                currency: "INR",
                name: "SANDGLASS",
                description: "Buy watches",
                image: "/user/assets/imgs/theme/sandglass-high-resolution-logo-transparent.svg",
                order_id: orderId,
                handler: function (response) {
                    payByRazorpay(response, totalPrice,id);
                },
                theme: {
                    color: "#000"
                }
            };

            const rzp1 = new Razorpay(options);
            rzp1.on('payment.failed', function (response) {
                handlePaymentFailure(response, totalPrice);
            });

            rzp1.open();
        }

        function payByRazorpay(response, totalPrice, id) {
            const verifySettings = {
                url: "/pay-by-razorpay",
                method: "PATCH",
                headers: {
                    "Content-Type": "application/json"
                },
                data: JSON.stringify({
                    response,
                    paymentMethod: 'Razorpay',
                    totalPrice,
                    id
                }),
            };

            $.ajax(verifySettings)
                .done(function (verificationResponse) {
                    Swal.fire({
                        title: "Success",
                        text: "Payment successful",
                        icon: "success"
                    }).then(() => {
                        window.location.href = `/order_details?orderId=${id}`;
                    });
                })
                .fail(function (error) {
                    console.error('Payment verification failed:', error);
                    Swal.fire({
                        title: 'Payment Verification Failed',
                        text: 'Your payment was not verified. Please try again.',
                        icon: 'error',
                        confirmButtonText: 'OK'
                    }).then(() => {
                        window.location.href = `/order_details?orderId=${id}`; // Redirect to cart page
                    });
                });
        }
    </script>
