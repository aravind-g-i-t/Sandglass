                                    <i class="text-primary material-icons md-place"></i>
                                </span>
                                <div class="text">
                                    <h6 class="mb-1">Deliver to</h6>
                                    <p class="mb-1">
                                        <%=address.address[0].addressLine1%>, <%=address.address[0].addressLine2%> <br><%=address.address[0].city%>, <%=address.address[0].state%> <br> Pincode: <%=address.address[0].pincode%> <br>Phone: <%=address.address[0].phone%>
                                    </p>
                                    <a href="/profile?tab=address">View address</a>
                                </div>
                            </article>
                        </div> <!-- col// -->
                    </div> <!-- row // -->
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th width="30%">Product</th>
                                            <th width="15%">Unit Price</th>
                                            <th width="15%">Quantity</th>
                                            
                                            <th width="15%" class="text-end">Total</th>
                                            <th width="15%">Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <%orderData.products.forEach(product=>{%>
                                        <tr>
                                            <td>
                                                <a class="itemside" href="#">
                                                    <span class="left">
                                                        <img src="/uploads/<%=product.productId.productImage[0]%>" class="img-xs" alt="Item">
                                                    </span>
                                                    <span class="info"><%=product.productId.productName%></span>
                                                </a>
                                            </td>
                                            <td> <%=product.productPrice%> </td>
                                            <td> <%=product.quantity%> </td>
                                            
                                            <td class="text-end"> <%=product.productPrice*product.quantity%> </td>
                                            <td class="text-<%=(product.status==='Cancelled')?'danger':'success'%>"> <%=product.status%> </td>
                                        </tr>
                                        <%})%>
                                        
                                        <tr>
                                            <td colspan="5">
                                                <article >
                                                    <dl class="dlist">
                                                        <dt>Subtotal:</dt>
                                                        <dd>₹ <%=totalPrice.toFixed(2)%></dd>
                                                    </dl>
                                                    <% if (orderData.coupon) { %>
                                                    <dl class="dlist">
                                                        <dt>Coupon discount (<%= orderData.coupon %>):</dt>
                                                        <dd>-₹ <%= ((totalPrice )* (couponDiscount / 100) ).toFixed(2)%></dd>
                                                    </dl>
                                                    <% }%>
                                                    <dl class="dlist">
                                                        <dt>Shipping cost:</dt>
                                                        <dd>₹ 300</dd>
                                                    </dl>
                                                    <dl class="dlist">
                                                        <dt>Grand total:</dt>
                                                        <dd> <b class="h5">₹ <%=orderData.payableAmount.toFixed(2)%></b> </dd>
                                                    </dl>
                                                    <dl class="dlist">
                                                        <dt class="text-muted">Payment status:</dt>
                                                        <dd>
                                                            <span class="badge rounded-pill alert-success text-success"><%=orderData.paymentStatus%></span>
                                                        </dd>
                                                    </dl>
                                                    <br>
                                                    <%if(orderData.paymentStatus==='Pending'){%>
                                                    <dl>
                                                        <button type="button" class="button-pay-now" id="payNowButton" onclick="showPaymentOptions()">Pay now</button>
                                                    </dl>
                                                    
                                                    <!-- Payment Options Container -->
                                                    <div id="paymentOptions" style="display: none;">
                                                        <h4>Payment Options</h4>
                                                        
                                                        <div class="form-check">
                                                            <input class="form-check-input" type="radio" name="payment-method" id="razorpay" value="RazorPay">
                                                            <label class="form-check-label" for="razorpay">
                                                                Razorpay
                                                            </label>
                                                        </div>
                                                        <div class="form-check">
                                                            <input class="form-check-input" type="radio" name="payment-method" id="wallet" value="Wallet" <% if (!walletApplicable) { %>disabled<% } %>>
                                                            <label class="form-check-label <% if (!walletApplicable) { %>text-muted<% } %>" for="wallet">
                                                                Wallet
                                                            </label>
                                                        </div>
                                                        <button class="btn btn-success my-3" onclick="payNow('<%=orderData.orderId%>','<%=orderData.payableAmount%>')">Proceed to pay</button>
                                                        <button class="btn btn-secondary my-3" onclick="cancelPayment()">Cancel</button>
                                                    </div>
                                                    <%}%>
                                                    

                                                </article>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div> <!-- table-responsive// -->
                            <%if(invoice){%>
                            <a class="btn btn-primary" href="/invoice/<%=orderData.orderId%>">Download Invoice</a>
                            <%}%>
