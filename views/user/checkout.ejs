        .address-card {
            border: 1px solid #ddd;
            padding: 15px;
            margin-bottom: 10px;
            display: flex;
            flex-direction: column;
            cursor: pointer;
            width: 100%; /* Make sure the div takes the full width of its container */
            max-width: 600px; /* Set a maximum width for the address cards */
        }
       
        .address-selection {
            display: flex;
            align-items: center;
            margin-bottom: 10px;
        }
        .address-selection input[type="radio"] {
            margin-right: 15px;
            width: 1.2em;
            height: 1.2em;
        }
        .selected {
            border-color: #007bff;
        }
        .cart-table img {
            width: 50px;
            height: 50px;
        }
        .cart-table th, .cart-table td {
            vertical-align: middle;
        }
        .summary-box {
            border: 1px solid #ddd;
            padding: 15px;
            margin-bottom: 20px;
        }
    </style>
</head>

