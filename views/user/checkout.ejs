    <script>
        
        function showNewAddressForm() {
            document.getElementById('new-address-form').style.display = 'block';
        }


        function cancelNewAddress() {
            document.getElementById('new-address-form').style.display = 'none';
            document.getElementById('address-form').reset();
        }


        function validateAndSaveAddress() {
            const form = document.getElementById('address-form');
            const fullname = document.getElementById('fullname').value.trim();
            const address1 = document.getElementById('address1').value.trim();
            const city = document.getElementById('city').value.trim();
            const state = document.getElementById('state').value.trim();
            const pincode = document.getElementById('pincode').value.trim();
            const phone = document.getElementById('phone').value.trim();
            const email = document.getElementById('email').value.trim();

            // Custom validation
            if (fullname === "" || address1 === "" || city === "" || state === "" || pincode === "" || phone === "" || email === "") {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'All required fields must be filled out.'
                });
                return;
            }

            if (!/^\d{6}$/.test(pincode)) {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Please enter a valid 6-digit pincode.'
                });
                return;
            }

            if (!/^\d{10}$/.test(phone)) {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Please enter a valid 10-digit phone number.'
                });
                return;
            }

            if (!/^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email)) {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Please enter a valid email address.'
                });
                return;
            }

            // If form is valid, submit it
            Swal.fire({
                icon: 'success',
                title: 'Success',
                text: 'Address saved successfully!'
            }).then(() => {
                form.submit();
            });
        }
    
        
    
        async function placeOrder() {
            const selectedAddress = document.querySelector('input[name="address"]:checked');
            if (!selectedAddress) {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Please select an address.'
                });
                return;
            }

            const selectedPaymentMethod = document.querySelector('input[name="payment-method"]:checked');
            if (!selectedPaymentMethod) {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Please select a payment method.'
                });
                return;
            }

            const addressId = selectedAddress.value;
            const paymentMethod = selectedPaymentMethod.value;
            const totalPriceElement = document.getElementById('total-payable');
            const totalPriceText = totalPriceElement.textContent; // or .innerText
            const totalPriceValue = totalPriceText.replace('₹', '').trim();
            const totalPrice = parseInt(totalPriceValue);

            if (paymentMethod === 'COD' || paymentMethod === "Wallet") {
                placeOrderCODorWallet(addressId, totalPrice, paymentMethod);
            } else {
                try {
                    const orderId = await createRazorpayOrder(totalPrice);
                    if (orderId) {
                        console.log('orderId is there')
                        handleRazorpayPayment(orderId, addressId, totalPrice);
                    }
                } catch (error) {
                    console.error('Error:', error);
                    Swal.fire({
                        title: 'Order not placed',
                        text: 'An unexpected error occurred. Please try again later.',
                        icon: 'error',
                        confirmButtonText: 'OK'
                    }).then(() => {
                        window.location.href = '/cart'; // Redirect to cart page
                    });
                }
            }
        }

        function placeOrderCODorWallet(addressId, totalPrice, paymentMethod) {
            fetch('/place-order', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    addressId: addressId,
                    paymentMethod: paymentMethod,
                    totalPrice: totalPrice
                })
            })
            .then(response => response.json())
            .then(data => {
                if (data.message === "Success") {
                    Swal.fire({
                        title: 'Order Successful',
                        text: 'Your order has been placed successfully!',
                        icon: 'success',
                        confirmButtonText: 'OK'
                    }).then(result => {
                        if (result.isConfirmed) {
                            window.location.href = '/'; // Replace '/' with your home page URL
                        }
                    });
                } else {
                    Swal.fire({
                        title: 'Order not placed',
                        text: data.message,
                        icon: 'error',
                        confirmButtonText: 'OK'
                    }).then(result => {
                        if (result.isConfirmed) {
                            window.location.href = '/cart'; // Redirect to cart page
                        }
                    });
                }
            })
            .catch(error => {
                console.error('Error:', error);
                Swal.fire({
                    title: 'Order not placed',
                    text: 'An unexpected error occurred. Please try again later.',
                    icon: 'error',
                    confirmButtonText: 'OK'
                }).then(() => {
                    window.location.href = '/cart'; // Redirect to cart page
                });
            });
        }

        async function createRazorpayOrder(totalPrice) {
            const settings = {
                url: "/payment/razorpay",
                method: "POST",
                headers: {
                    "Content-Type": "application/json"
                },
                data: JSON.stringify({
                    amount: totalPrice * 100 // Convert to paisa
                }),
            };

            const response = await $.ajax(settings);
            return response.orderId;
        }

        function handleRazorpayPayment(orderId, addressId, totalPrice) {
            const options = {
                key: "rzp_test_7DylGE19pXY4Lf", // Replace with your Razorpay Key ID
                amount: totalPrice * 100, // Amount in paisa
                currency: "INR",
                name: "SANDGLASS",
                description: "Buy watches",
                image: "/user/assets/imgs/theme/sandglass-high-resolution-logo-transparent.svg",
                order_id: orderId,
                handler: function (response) {
                    verifyRazorpayPayment(response, addressId, totalPrice);
                },
                theme: {
                    color: "#000"
                }
            };

            const rzp1 = new Razorpay(options);
            rzp1.on('payment.failed', function (response) {
                handlePaymentFailure(response, addressId, totalPrice);
            });

            rzp1.open();
        }

        function verifyRazorpayPayment(response, addressId, totalPrice) {
            const verifySettings = {
                url: "/verifypayment",
                method: "POST",
                headers: {
                    "Content-Type": "application/json"
                },
                data: JSON.stringify({
                    response,
                    addressId,
                    paymentMethod: 'Razorpay',
                    totalPrice
                }),
            };

            $.ajax(verifySettings)
                .done(function (verificationResponse) {
                    Swal.fire({
                        title: "Success",
                        text: "Payment successful",
                        icon: "success"
                    }).then(() => {
                        window.location.href = "/";
                    });
                })
                .fail(function (error) {
                    console.error('Payment verification failed:', error);
                    Swal.fire({
                        title: 'Payment Verification Failed',
                        text: 'Your payment was not verified. Please try again.',
                        icon: 'error',
                        confirmButtonText: 'OK'
                    }).then(() => {
                        window.location.href = '/cart'; // Redirect to cart page
                    });
                });
        }

        function handlePaymentFailure(response, addressId, totalPrice) {
            
            placeOrderCODorWallet (addressId, totalPrice,paymentMethod='COD');
                
            
        }


          
